number = math.pow(2, 3)
guess = 0

say("Guess the number between 1 and 10:")

for i in 1..3 {
    say("Enter your guess:")
    guess = i + 5
    if guess == number {
        say("Correct! You win!")
        break
    } else {
        say("Wrong guess. Try again.")
    }
}
say("Game over. The number was: " + number)
